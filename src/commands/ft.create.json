            "type": "pure-token",
            "token": "ON"
          },
          {
            "name": "type",
            "type": "oneof",
            "arguments": [
              {
                "name": "hash",
                "type": "pure-token",
                "token": "HASH"
              },
              {
                "name": "json",
                "type": "pure-token",
                "token": "JSON"
              }
            ]
          }
        ]
      },
      {
