                            "arguments": [
                              {
                                "name": "type_token",
                                "type": "pure-token",
                                "token": "TYPE"
                              },
                              {
                                "name": "format",
                                "type": "pure-token",
                                "token": "FLOAT32"
                              }
                            ]
                          },
                          {
                            "name": "dim",
                            "type": "block",
                            "arguments": [
                              {
                                "name": "dim_token",
                                "type": "pure-token",
                                "token": "DIM"
                              },
                              {
                                "name": "value",
                                "type": "integer"
                              }
                            ]
                          },
                          {
                            "name": "distance_metric",
                            "type": "block",
                            "arguments": [
                              {
                                "name": "dm_token",
                                "type": "pure-token",
                                "token": "DISTANCE_METRIC"
